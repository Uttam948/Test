package NovermberCoding_2025;

public class Feb24 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 0;
		int b = 1;
		
		for(int i = 0; i<7 ; i++) {
			
			int c = a + b;
			b = a;
			a = c;
			
			//System.out.println(c);
		}

		
		
	}
	
	
	public static int recurr(int n) {
		
		if(n == 0 || n ==1) {
			return n;
		}
		
		return recurr(n-1) + recurr(n-2);
	}

}
