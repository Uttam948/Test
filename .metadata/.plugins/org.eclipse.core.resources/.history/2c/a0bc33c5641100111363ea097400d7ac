package NovermberCoding_2025;

import java.util.InputMismatchException;
import java.util.Scanner;

public class Feb16 {

	
	public static void check(String str) {
		
		if(str.length() > 10) {
			throw new IllegalArgumentException("Please enter number up to 10 digit");
		}else if(str.length() < 10 ) {
			throw new IllegalAccessError("Please enter number less then up to 10 digit");
		}else {
			
			for(int i =0 ; i<str.length() ; i++) {
				if(!Character.isDigit(str.charAt(i))) {
					throw new InputMismatchException("Please provide the correct input");
				}
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
           
		Scanner sc = new Scanner(System.in);
		
		String str = sc.nextLine();
		
		try {
			check(str);
		}catch(Exception e) {
			System.out.println("IllegalAccessError : " + e.getMessage());
		}
		
		try {
			int arr[] = new int[10];
			System.out.println("there is no any error");
			int a = 0/10;
		}catch(ArrayIndexOutOfBoundsException e){
			System.out.println("Array index does not exist: " + e.getMessage());
		}catch ()
	}

}
