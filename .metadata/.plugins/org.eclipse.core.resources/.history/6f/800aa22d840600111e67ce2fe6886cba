package NovermberCoding_2025;

public class Feb10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         int arr[] = {1,2,3,4};
         
         for(int i = 0 ; i < arr.length ; i++) {
        	 
        	 arr[i] = arr[i] -1;
         }
         
         for(int i =0 ; i<arr.length ; i++) {
        	 
        	 System.out.println(arr[i]);
         }
	}

}
