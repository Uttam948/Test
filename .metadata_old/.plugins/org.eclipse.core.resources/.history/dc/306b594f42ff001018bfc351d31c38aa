package NovermberCoding_2025;

import java.util.Collection;
import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;
import java.util.function.Function;
import java.util.stream.Collectors;

public class Feb01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String str = "javafullstack";
		
		Map<Character , Long> map = new HashMap<>();
		
		map = str.chars().mapToObj( c -> (char) c).collect(Collectors.groupingBy(Function.identity() , Collectors.counting()));
		
		Long max = 0L;
		Long secmax = 0L;
		char ch = ' ';
		for(Entry<Character, Long> i : map.entrySet()) {
			
			if(i.getValue() > max) {
				secmax = max;
				max = i.getValue();
				
			} 
			if(i.getValue() > secmax && i.getValue() < max) {
				
				secmax = i.getValue();
				ch = i.getKey();
			}
		}
		
		System.out.println(map.containsValue(secmax));

	}

}
