
public class PatternOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k = 10;
		
		for(int i =0 ;i<5 ; i++) {
			
			for(int j = 0;j <= i ; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		for(int i =0 ;i<5;i++) {
			
			for(int j = 6; j<=10 ; j++) {
				if(k <= j) {
					System.out.print("*");
				}else {
					System.out.print(" ");
				}
			}
			k--;
			System.out.println();
		}

	}

}
