
public class PatternOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k = 5;
		for(int i =0 ;i<5;i++) {
			
			for(int j = 0; j<=5 ; j++) {
				if( j < k) {
					System.out.print("*");
				}else {
					System.out.print(" ");
				}
			}
			k--;
			System.out.println();
		}

	}

}
